<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kh.deli.domain.owner.mapper.OwnerOwnerMapper">
    <select id="bsInfo" resultType="kh.deli.global.entity.OwnerDTO">
        SELECT
        OWNER.*
        FROM OWNER,STORE
        WHERE OWNER.OWNER_SEQ=STORE.OWNER_SEQ
    </select>
</mapper>
